{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Debug",
      "Microsoft": "Debug",
      "Microsoft.Hosting.Lifetime": "Debug"
    }
  },
  "AllowedHosts": "*",
  "HttpClientConfig": {
    "BaseAddress": "https://api.digikala.com",
    "UserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:114.0) Gecko/20100101 Firefox/114.0",
    "Referrer": "https://api.digikala.com"
  },
  "ApiConfig": {
    "ItemsHistoryFileName": "items-history.json",
    "ItemsHistoryInDays": 2,
    "AllowedRetries": 3,
    "OutputFolderName": "processed-items",
    "ApiUrls": [
      "v1/fresh-offers/products/?seo_url=&page=$page",
      "v1/incredible-offers/products/?seo_url=&page=$page"
    ],
    "MaxApiPagesToSearch": 100,
    "TaskDelayInSeconds": 10
  }
}
